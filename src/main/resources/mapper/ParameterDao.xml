<!-- 设置为IUserDao接口方法提供sql语句配置 -->
<mapper namespace="com.main.dao.ParameterDao">

    <select id="selectParameterById" resultType="Parameter" parameterType="long">
        SELECT * FROM parameter WHERE id = #{id}
    </select>


